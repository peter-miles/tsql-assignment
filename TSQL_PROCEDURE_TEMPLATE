USE TSQL_ASSIGNMENT;
GO

IF OBJECT_ID('INSERT_NAME') IS NOT NULL
DROP PROCEDURE INSERT_NAME;
GO 

CREATE PROCEDURE INSERT_NAME AS 
BEGIN
    BEGIN TRY
        
    END TRY
    BEGIN CATCH
        BEGIN
            DECLARE @ERRORMESSAGE NVARCHAR(MAX) = ERROR_MESSAGE();
            THROW 50000, @ERRORMESSAGE, 1
        END; 
    END CATCH
END;
GO

EXEC INSERT_NAME;

SELECT * FROM TABLE_NAME;